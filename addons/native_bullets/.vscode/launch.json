  "version": "0.2.0",
  "configurations": [
    {
      "name": "(Windows) Launch",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "godot",
      "symbolSearchPath": "${workspaceFolder}\\bin\\win64",
